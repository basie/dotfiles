{
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "standard"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "standard",
    "promise",
    "jest"
  ],
  "rules": {
    "no-console": "off",
    "no-multiple-empty-lines": [
      "error", { "max": 1, "maxEOF": 0, "maxBOF": 0 }
    ],
    "react/prop-types": "off"
  }
}
